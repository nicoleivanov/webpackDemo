{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ],
    "stage-2",
    "flow",
    "react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "^webpackDemo/(.+)": "./src/\\1"
        }
      }
    ]
  ]
}
